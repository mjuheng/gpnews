}
.send_message {
        background-color: #EFEFF4;
		padding-left: 20rpx;
		padding-right: 20rpx;
		height: 1334rpx;
}
view{
        font-size: 28rpx;
}
    /*问题反馈*/
.feedback-title {
    	display: -webkit-box;
    	display: -webkit-flex;
    	display: flex;
    	-webkit-box-orient: horizontal;
    	-webkit-box-direction: normal;
    	-webkit-flex-direction: row;
    	        flex-direction: row;
    	-webkit-box-pack: justify;
    	-webkit-justify-content: space-between;
    	        justify-content: space-between;
    	-webkit-box-align: center;
    	-webkit-align-items: center;
    	        align-items: center;
    	color: #8f8f94;
    	font-size: 28rpx;
}
.feedback-body {
    	background: #fff;
}
.feedback-textare {
    	height: 200rpx;
    	font-size: 34rpx;
    	line-height: 50rpx;
    	width: 100%;
    	box-sizing: border-box;
    	padding: 20rpx 30rpx 0;
}
.feedback-input {
    	font-size: 34rpx;
    	height: 60rpx;
    	min-height: 50rpx;
    	padding: 15rpx 20rpx;
    	line-height: 50rpx;
}
.feedback-submit {
    	background: #007AFF;
    	color: #FFFFFF;
		margin-top: 40rpx;
}

